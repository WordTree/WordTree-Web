#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_vue-demi@0.12.1@vue-demi/bin/vue-demi-switch.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_vue-demi@0.12.1@vue-demi/bin/vue-demi-switch.js" "$@"
  ret=$?
fi
exit $ret
